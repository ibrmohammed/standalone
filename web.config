<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add
        name="aspNetCore"
        path="*"
        verb="*"
        modules="AspNetCoreModule"
        resourceType="Unspecified"/>
    </handlers>
    <aspNetCore
        processPath=".\standalone.exe"
        arguments=""
        stdoutLogEnabled="true"
        stdoutLogFile=".\logs\standalone"
        startupTimeLimit="300"/>
  </system.webServer>
</configuration>